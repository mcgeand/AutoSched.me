// This is the initial Prisma schema for AutoSched.me
// See AUT-48 for context

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String?
  createdAt DateTime @default(now())
}
